{
  "extends": "../../tsconfig.json",
  "include": ["./src/**/*", ".next/types/**/*.ts"],
  "compilerOptions": {
    "outDir": "./dist/types",
    "emitDeclarationOnly": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"]
}
