    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@types/jest": "^29.1.1",
    "esbuild": "^0.17.0",
    "jest": "^29.1.2",
    "rollup": "2.78.0",
    "rollup-plugin-esbuild": "4.9.3",
    "ts-jest": "^29.0.3",
    "typescript": "4.8.2"
