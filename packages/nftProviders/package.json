    "url": "git+https://github.com/oort-digital/oort-common.git"
  },
  "scripts": {
    "build:types": "tsc",
    "build:source": "rollup --config rollup.config.js",
    "build": "yarn build:source && yarn build:types",
    "test": "jest",
    "test:debug": "node --inspect-brk ../../node_modules/jest/bin/jest.js",
    "rm": "rimraf node_modules"
  },
  "jest": {
    "verbose": true,
    "resetMocks": false,
