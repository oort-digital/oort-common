{
  "extends": "../../tsconfig.json",
  "exclude": ["**/*.stories.tsx", "node_modules"],
  "include": ["src/**/*", "typings.d.ts", ".next/types/**/*.ts"],
  "compilerOptions": {
    "outDir": "./dist",
    "emitDeclarationOnly": true,
    "jsx": "preserve",
    "composite": false,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
